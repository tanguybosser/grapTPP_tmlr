for split in 0
do
python3 -u scripts/train2.py --no-mlflow --dataset 'mooc_filtered' --load-from-dir '../neuralTPPs/data/baseline3' \
--save-results-dir 'results/comput/mooc_filtered' --save-check-dir 'checkpoints/comput/mooc_filtered' \
--eval-metrics True --include-poisson False --patience 51 --batch-size 32 --split $split \
